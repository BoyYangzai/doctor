    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx}": [
